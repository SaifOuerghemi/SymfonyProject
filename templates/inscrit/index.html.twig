{% extends 'base.html.twig' %}

{% block title %}Inscrit index{% endblock %}

{% block body %}
    <h1>Inscrit index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Formation</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for inscrit in inscrits %}
            <tr>
                <td>{{ inscrit.id }}</td>
                <td>{{ inscrit.getFormationId.getDescription }}</td>
                <td>
                    <a href="{{ path('inscrit_edit', {'id': inscrit.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
